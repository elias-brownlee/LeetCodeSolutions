class Solution(object):
    def convert(self, s, numRows):
        """
        :type s: str
        :type numRows: int
        :rtype: str
        """
        if numRows == 1:
            return s
        rows = [""] * numRows
        currRow = 0
        inc = 1
        for letter in s:
            rows[currRow] += letter
            if currRow == numRows-1:
                inc = -1
            elif currRow == 0:
                inc = 1
            currRow += inc
        return "".join(rows)
